   "source": [
    "import cv2\n",
    "import numpy as np\n",
    "img=cv2.imread('./images/input.jpg')\n",
    "M=np.ones(img.shape,dtype=\"uint8\")*75\n",
    "addmat=cv2.add(img,M)\n",
    "cv2.imshow(\"added\",addmat)\n",
    "cv2.waitKey()\n",
    "\n",
    "submat=cv2.subtract(img,M)\n",
    "cv2.imshow(\"added\",submat)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
